    <sql id="selectEsServicerVo">
        select id, user_id, name, type_id, account_phone, corporate, service_type_Xx, service_type_path, manage_form, region_pre, region_xx, region_id, introduce, logo, video, manage_cert, picture_urls, population, income, serve_num, farmers_num, serve_area, raise_num, water_num, other_num, crop_area, is_auth, create_by, create_time, update_by, update_time from es_servicer
    </sql>

    <select id="selectEsServicerList" parameterType="EsServicer" resultMap="EsServicerResult">
        <include refid="selectEsServicerVo"/>
        <where>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="typeId != null "> and type_id = #{typeId}</if>
            <if test="accountPhone != null  and accountPhone != ''"> and account_phone = #{accountPhone}</if>
            <if test="corporate != null  and corporate != ''"> and corporate = #{corporate}</if>
            <if test="serviceTypeXx != null  and serviceTypeXx != ''"> and service_type_Xx = #{serviceTypeXx}</if>
            <if test="serviceTypePath != null  and serviceTypePath != ''"> and service_type_path = #{serviceTypePath}</if>
            <if test="manageForm != null "> and manage_form = #{manageForm}</if>
            <if test="regionPre != null  and regionPre != ''"> and region_pre = #{regionPre}</if>
            <if test="regionXx != null  and regionXx != ''"> and region_xx = #{regionXx}</if>
            <if test="regionId != null  and regionId != ''"> and region_id = #{regionId}</if>
            <if test="introduce != null  and introduce != ''"> and introduce like concat('%', #{introduce}, '%')</if>
            <if test="logo != null  and logo != ''"> and logo = #{logo}</if>
            <if test="video != null  and video != ''"> and video = #{video}</if>
            <if test="manageCert != null  and manageCert != ''"> and manage_cert = #{manageCert}</if>
            <if test="pictureUrls != null  and pictureUrls != ''"> and picture_urls = #{pictureUrls}</if>
            <if test="population != null "> and population &gt;= #{population}</if>
            <if test="params.beginIncome != null and params.beginIncome != '' and params.endIncome != null and params.endIncome != ''"> and income between #{params.beginIncome} and #{params.endIncome}</if>
            <if test="params.beginServeNum != null and params.beginServeNum != '' and params.endServeNum != null and params.endServeNum != ''"> and serve_num between #{params.beginServeNum} and #{params.endServeNum}</if>
            <if test="params.beginFarmersNum != null and params.beginFarmersNum != '' and params.endFarmersNum != null and params.endFarmersNum != ''"> and farmers_num between #{params.beginFarmersNum} and #{params.endFarmersNum}</if>
            <if test="params.beginServeArea != null and params.beginServeArea != '' and params.endServeArea != null and params.endServeArea != ''"> and serve_area between #{params.beginServeArea} and #{params.endServeArea}</if>
            <if test="params.beginRaiseNum != null and params.beginRaiseNum != '' and params.endRaiseNum != null and params.endRaiseNum != ''"> and raise_num between #{params.beginRaiseNum} and #{params.endRaiseNum}</if>
            <if test="params.beginWaterNum != null and params.beginWaterNum != '' and params.endWaterNum != null and params.endWaterNum != ''"> and water_num between #{params.beginWaterNum} and #{params.endWaterNum}</if>
            <if test="params.beginOtherNum != null and params.beginOtherNum != '' and params.endOtherNum != null and params.endOtherNum != ''"> and other_num between #{params.beginOtherNum} and #{params.endOtherNum}</if>
            <if test="params.beginCropArea != null and params.beginCropArea != '' and params.endCropArea != null and params.endCropArea != ''"> and crop_area between #{params.beginCropArea} and #{params.endCropArea}</if>
            <if test="isAuth != null "> and is_auth = #{isAuth}</if>
        </where>
    </select>

