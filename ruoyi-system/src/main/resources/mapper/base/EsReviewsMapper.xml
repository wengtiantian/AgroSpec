<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.base.mapper.EsReviewsMapper">
    
    <resultMap type="EsReviews" id="EsReviewsResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="servicerId"    column="servicer_id"    />
        <result property="content"    column="content"    />
        <result property="rating"    column="rating"    />
        <result property="isDelete"    column="is_delete"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="SQdim"    column="SQdim"    />
        <result property="SenScore"    column="SenScore"    />
    </resultMap>

    <sql id="selectEsReviewsVo">
        select id, user_id, servicer_id, content, rating, is_delete, create_by, create_time, update_by, update_time, SQdim, SenScore from es_reviews
    </sql>

    <select id="selectEsReviewsList" parameterType="EsReviews" resultMap="EsReviewsResult">
        <include refid="selectEsReviewsVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="servicerId != null "> and servicer_id = #{servicerId}</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="isDelete != null "> and is_delete = #{isDelete}</if>
        </where>
    </select>
    
    <select id="selectEsReviewsById" parameterType="Long" resultMap="EsReviewsResult">
        <include refid="selectEsReviewsVo"/>
        where id = #{id}
    </select>

    <insert id="insertEsReviews" parameterType="EsReviews" useGeneratedKeys="true" keyProperty="id">
        insert into es_reviews
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="servicerId != null">servicer_id,</if>
            <if test="content != null">content,</if>
            <if test="rating != null">rating,</if>
            <if test="isDelete != null">is_delete,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="SQdim != null">SQdim,</if>
            <if test="SenScore != null">SenScore,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="servicerId != null">#{servicerId},</if>
            <if test="content != null">#{content},</if>
            <if test="rating != null">#{rating},</if>
            <if test="isDelete != null">#{isDelete},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="SQdim != null">#{SQdim},</if>
            <if test="SenScore != null">#{SenScore},</if>
         </trim>
    </insert>

    <update id="updateEsReviews" parameterType="EsReviews">
        update es_reviews
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="servicerId != null">servicer_id = #{servicerId},</if>
            <if test="content != null">content = #{content},</if>
            <if test="rating != null">rating = #{rating},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="SQdim != null">SQdim = #{SQdim},</if>
            <if test="SenScore != null">SenScore = #{SenScore},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteEsReviewsById" parameterType="Long">
        delete from es_reviews where id = #{id}
    </delete>

    <delete id="deleteEsReviewsByIds" parameterType="String">
        delete from es_reviews where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>